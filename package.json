{
	"name": "@prismicio/simulator",
	"version": "0.1.3",
	"description": "Preview and develop Prismic slices fast with minimal configuration",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/github_org_slash_github_repo.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./kit": {
			"require": {
				"types": "./dist/kit/index.d.ts",
				"default": "./dist/kit.cjs"
			},
			"import": {
				"types": "./dist/kit/index.d.ts",
				"default": "./dist/kit.js"
			}
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"./src"
	],
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"kit": [
				"dist/kit/index.d.ts"
			]
		}
	},
	"scripts": {
		"build": "vite build",
		"dev": "nuxi dev playground",
		"format": "prettier --write .",
		"prepare": "npm run build && nuxi prepare playground",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:dry": "standard-version --dry-run",
		"release:alpha": "npm run test && standard-version --release-as major --prerelease alpha && git push --follow-tags && npm run build && npm publish --tag alpha",
		"release:alpha:dry": "standard-version --release-as major --prerelease alpha --dry-run",
		"lint": "eslint --ext .js,.ts .",
		"types": "vitest typecheck --run && tsc --noEmit",
		"types:watch": "vitest typecheck",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit && npm run build && npm run size"
	},
	"dependencies": {
		"@prismicio/client": "^7.0.1",
		"@types/statuses": "^2.0.1",
		"statuses": "^2.0.1"
	},
	"devDependencies": {
		"@nuxt/eslint-config": "^0.1.1",
		"@nuxtjs/prismic": "^3.0.0",
		"@nuxtjs/tailwindcss": "^6.7.0",
		"@size-limit/preset-small-lib": "^8.2.4",
		"@slicemachine/adapter-nuxt": "^0.3.0",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/node": "^18",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"@vitest/coverage-v8": "^0.32.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"jsdom": "^22.1.0",
		"nuxt": "^3.5.3",
		"prettier": "^2.8.8",
		"prettier-plugin-jsdoc": "^0.4.2",
		"size-limit": "^8.2.4",
		"standard-version": "^9.5.0",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-sdk": "^0.1.1",
		"vitest": "^0.32.0"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
